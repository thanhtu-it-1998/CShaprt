<Page
    x:Class="Lap19.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Lap19"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <StackPanel>
        <StackPanel Orientation="Horizontal" Margin="0,20,20,0">
            <Button Content="Home" Name="HomeButton" Width="100" Margin="20,0,20,0" Click="HomeButton_Click"/>
            <Button Content="Back" Name="BackButton" Width="100" Margin="0,0,20,0" Click="BackButton_Click"/>
            <Button Content="Forward" Name="ForwardButton" Width="100" Margin="0,0,20,0" Click="ForwardButton_Click"/>
        </StackPanel>
        <Frame x:Name="myFrane">

        </Frame>
    </StackPanel>
</Page>
